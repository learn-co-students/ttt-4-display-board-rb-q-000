# Define display_board that accepts a board and prints
# out the current state.

def display_board(board)
    i = -1
    puts " #{board[i+=1]} | #{board[i+=1]} | #{board[i+=1]}"
    puts "-----------"
    puts " #{board[i+=1]} | #{board[i+=1]} | #{board[i+=1]}"
    puts "-----------"
    puts " #{board[i+=1]} | #{board[i+=1]} | #{board[i+=1]}"
end
